---
layout: "default"
---
<article class="post">
  <header class="post-header">
    <h1 class="post-title">{{ title }}</h1>
    <time class="post-date" datetime="{{ page.date | date('yyyy-MM-dd') }}">
      Published on {{ page.date | date('dd LLLL yyyy') }}
    </time>
  </header>

  <div class="post-content">
    {{ content | safe }}
  </div>
</article>